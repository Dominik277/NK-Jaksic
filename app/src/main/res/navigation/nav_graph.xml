<?xml version="1.0" encoding="utf-8"?>
<navigation xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/nav_graph"
    app:startDestination="@id/nav_pocetnaStranica">

    <fragment
        android:id="@+id/nav_momcad"
        android:name="hr.fragmenti.fragmenti.MomcadFragment"
        android:label="Momčad"
        tools:layout="@layout/fragment_momcad">
        <action
            android:id="@+id/action_nav_momcad_to_detailMomcadFragment"
            app:destination="@id/detailMomcadFragment" />
    </fragment>
    <fragment
        android:id="@+id/najboljiStrijelciFragment"
        android:name="hr.fragmenti.fragmenti.NajboljiStrijelciFragment"
        android:label="Najbolji Strijelci"
        tools:layout="@layout/fragment_najbolji_strijelci"/>
    <fragment
        android:id="@+id/nav_pocetnaStranica"
        android:name="hr.fragmenti.fragmenti.PocetnaStranicaFragment"
        android:label="Početna stranica"
        tools:layout="@layout/fragment_pocetna_stranica"/>
    <fragment
        android:id="@+id/nav_raspored"
        android:name="hr.fragmenti.fragmenti.RasporedFragment"
        android:label="Raspored"
        tools:layout="@layout/fragment_raspored">
        <action
            android:id="@+id/action_nav_raspored_to_detailRasporedFragment"
            app:destination="@id/detailRasporedFragment" />
    </fragment>
    <fragment
        android:id="@+id/nav_rezultati"
        android:name="hr.fragmenti.fragmenti.RezultatiFragment"
        android:label="Rezultati"
        tools:layout="@layout/fragment_rezultati">
        <action
            android:id="@+id/action_nav_rezultati_to_detailRezultatiFragment"
            app:destination="@id/detailRezultatiFragment" />
    </fragment>
    <fragment
        android:id="@+id/nav_tablica"
        android:name="hr.fragmenti.fragmenti.TablicaFragment"
        android:label="Tablica"
        tools:layout="@layout/fragment_tablica">
        <action
            android:id="@+id/action_nav_tablica_to_najboljiStrijelciFragment"
            app:destination="@id/najboljiStrijelciFragment" />
        <action
            android:id="@+id/action_nav_tablica_to_tablicaRezultatiFragment"
            app:destination="@id/tablicaRezultatiFragment" />
        <action
            android:id="@+id/action_nav_tablica_to_tablicaTablicaFragment"
            app:destination="@id/tablicaTablicaFragment" />
        <action
            android:id="@+id/action_nav_tablica_to_tablicaRasporedFragment"
            app:destination="@id/tablicaRasporedFragment" />
    </fragment>
    <fragment
        android:id="@+id/tablicaRasporedFragment"
        android:name="hr.fragmenti.fragmenti.TablicaRasporedFragment"
        android:label="Raspored u ligi"
        tools:layout="@layout/tablica_fragment_raspored"/>
    <fragment
        android:id="@+id/tablicaRezultatiFragment"
        android:name="hr.fragmenti.fragmenti.TablicaRezultatiFragment"
        android:label="Rezultati u ligi"
        tools:layout="@layout/novi_fragment_rezultati"/>
    <fragment
        android:id="@+id/tablicaTablicaFragment"
        android:name="hr.fragmenti.fragmenti.TablicaTablicaFragment"
        android:label="Tablica"
        tools:layout="@layout/tablica_fragment_tablica"/>
    <fragment
        android:id="@+id/nav_vijesti"
        android:name="hr.fragmenti.fragmenti.VijestiFragment"
        android:label="Vijesti"
        tools:layout="@layout/fragment_vijesti">
        <action
            android:id="@+id/action_nav_vijesti_to_detailVijestiFragment"
            app:destination="@id/detailVijestiFragment" />
    </fragment>
    <fragment
        android:id="@+id/updateNajboljiStrijalacFragment"
        android:name="hr.fragmenti.updateDelete.UpdateNajboljiStrijalacFragment"
        android:label="Ažuiraj strijelca"
        tools:layout="@layout/update_strijelci_fragment">
        <action
            android:id="@+id/action_updateNajboljiStrijalacFragment_to_najboljiStrijelciFragment"
            app:destination="@id/najboljiStrijelciFragment" />
        <argument
            android:name="updateStrijelciArgs"
            app:argType="hr.database.table.NajboljiStrijelci" />
    </fragment>
    <fragment
        android:id="@+id/updateRasporedFragment"
        android:name="hr.fragmenti.updateDelete.UpdateRasporedFragment"
        android:label="Ažuriraj raspored"
        tools:layout="@layout/update_raspored_fragment">
        <action
            android:id="@+id/action_updateRasporedFragment_to_rasporedFragment"
            app:destination="@id/nav_raspored" />
        <argument android:name="updateRasporedArgs"
            app:argType="hr.database.table.Raspored" />
    </fragment>
    <fragment
        android:id="@+id/updateTablicaTablicaFragment"
        android:name="hr.fragmenti.updateDelete.UpdateTablicaTablicaFragment"
        android:label="Ažuriraj tablicu"
        tools:layout="@layout/update_tablica_tablica_fragment">
        <action
            android:id="@+id/action_updateTablicaTablicaFragment_to_tablicaTablicaFragment"
            app:destination="@id/tablicaTablicaFragment" />
        <argument android:name="updateTablicaArgs"
            app:argType="hr.database.table.TablicaTablica" />
    </fragment>
    <fragment
        android:id="@+id/updateRezultatFragment"
        android:name="hr.fragmenti.updateDelete.UpdateRezultatFragment"
        android:label="Ažuriraj rezultat"
        tools:layout="@layout/update_rezultat_fragment">
        <action
            android:id="@+id/action_updateRezultatFragment_to_rezultatiFragment"
            app:destination="@id/nav_rezultati" />
        <argument
            android:name="updateRezultatiArgs"
            app:argType="hr.database.table.Rezultat" />
    </fragment>
    <fragment
        android:id="@+id/updateMomcadFragment"
        android:name="hr.fragmenti.updateDelete.UpdateMomcadFragment"
        android:label="Ažuriraj momčad"
        tools:layout="@layout/update_momcad_fragment">
        <action
            android:id="@+id/action_updateMomcadFragment_to_momcadFragment"
            app:destination="@id/nav_momcad" />
        <argument
            android:name="updateMomcadArgs"
            app:argType="hr.database.table.Igraci" />
    </fragment>
    <fragment
        android:id="@+id/updateTablicaRezultatFragment"
        android:name="hr.fragmenti.updateDelete.UpdateTablicaRezultatFragment"
        android:label="Ažuriraj rezultate u ligi"
        tools:layout="@layout/update_tablica_rezultat_fragment">
        <action
            android:id="@+id/action_updateTablicaRezultatFragment_to_tablicaRezultatiFragment"
            app:destination="@id/tablicaRezultatiFragment" />
        <argument
            android:name="updateRezultatiLigaArgs"
            app:argType="hr.database.table.TablicaRezultati" />
    </fragment>
    <fragment
        android:id="@+id/updateTablicaRasporedFragment"
        android:name="hr.fragmenti.updateDelete.UpdateTablicaRasporedFragment"
        android:label="Ažuriraj raspored u ligi"
        tools:layout="@layout/update_tablica_raspored_fragment">
        <action
            android:id="@+id/action_updateTablicaRasporedFragment_to_tablicaRasporedFragment"
            app:destination="@id/tablicaRasporedFragment" />
        <argument
            android:name="updateRasporedLigaArgs"
            app:argType="hr.database.table.TablicaRaspored" />
    </fragment>
    <fragment
        android:id="@+id/dodajNoviRaspored"
        android:name="hr.fragmenti.novoDodavanje.DodajNoviRasporedFragment"
        android:label="Dodaj novi raspored"
        tools:layout="@layout/fragment_novi_raspored">
        <action
            android:id="@+id/action_dodajNoviRasporedFragment_to_rasporedFragment"
            app:destination="@id/nav_raspored" />
    </fragment>
    <fragment
        android:id="@+id/dodajNovogStrijelca"
        android:name="hr.fragmenti.novoDodavanje.DodajNovogStrijelcaFragment"
        android:label="Dodaj novog strijelca"
        tools:layout="@layout/fragment_novi_strijelac">
        <action
            android:id="@+id/action_dodajNovogStrijelcaFragment_to_najboljiStrijelciFragment"
            app:destination="@id/najboljiStrijelciFragment" />
    </fragment>
    <fragment
        android:id="@+id/dodajTablicuRapored"
        android:name="hr.fragmenti.novoDodavanje.DodajNoviRasporedLigaFragment"
        android:label="Dodaj novi raspored u ligi"
        tools:layout="@layout/fragment_novi_raspored_liga">
        <action
            android:id="@+id/action_dodajNoviRasporedLigaFragment_to_tablicaRasporedFragment"
            app:destination="@id/tablicaRasporedFragment" />
    </fragment>
    <fragment
        android:id="@+id/dodajNoviRezultat"
        android:name="hr.fragmenti.novoDodavanje.DodajNoviRezultatFragment"
        android:label="Dodaj novi rezultat u ligi"
        tools:layout="@layout/fragment_novi_rezultat">
        <action
            android:id="@+id/action_dodajNoviRezultatFragment_to_rezultatiFragment"
            app:destination="@id/nav_rezultati" />
    </fragment>
    <fragment
        android:id="@+id/dodajNovogIgraca"
        android:name="hr.fragmenti.novoDodavanje.DodajNovogIgraca"
        android:label="Dodaj novog igrača"
        tools:layout="@layout/fragment_novi_igrac">
        <action
            android:id="@+id/action_dodajNovogIgraca2_to_momcadFragment"
            app:destination="@id/nav_momcad" />
    </fragment>
    <fragment
        android:id="@+id/dodajTablicuRezultat"
        android:name="hr.fragmenti.novoDodavanje.DodajNoviRezultatLigaFragment"
        android:label="Dodaj novi rezultat u ligi"
        tools:layout="@layout/fragment_novi_rezultati_liga">
        <action
            android:id="@+id/action_dodajNoviRezultatLigaFragment_to_tablicaRezultatiFragment"
            app:destination="@id/tablicaRezultatiFragment" />
    </fragment>
    <fragment
        android:id="@+id/dodajNovuTablicu"
        android:name="hr.fragmenti.novoDodavanje.DodajNovuTablicuFragment"
        android:label="Dodaj novu Tablicu"
        tools:layout="@layout/fragment_nova_tablica">
        <action
            android:id="@+id/action_dodajNovuTablicu_to_tablicaTablicaFragment"
            app:destination="@id/tablicaTablicaFragment" />
    </fragment>
    <fragment
        android:id="@+id/dodajNovuVijest"
        android:name="hr.fragmenti.novoDodavanje.DodajNovuVijest"
        android:label="Dodaj novu vijest"
        tools:layout="@layout/fragment_nova_vijest">
        <action
            android:id="@+id/action_dodajNovuVijest2_to_vijestiFragment"
            app:destination="@id/nav_vijesti" />
    </fragment>
    <fragment
        android:id="@+id/updateVijestiFragment"
        android:name="hr.fragmenti.updateDelete.UpdateVijestiFragment"
        android:label="Ažuriraj vijesti"
        tools:layout="@layout/update_vijesti_fragment">
        <action
            android:id="@+id/action_updateVijestiFragment_to_nav_vijesti"
            app:destination="@id/nav_vijesti" />
        <argument
            android:name="updateVijestiArgs"
            app:argType="hr.database.table.Vijesti" />
    </fragment>
    <fragment
        android:id="@+id/detailVijestiFragment"
        android:name="hr.fragmenti.detailFragmenti.DetailVijestiFragment"
        android:label="Detalji o vijesti"
        tools:layout="@layout/detail_vijesti_fragment">
        <argument
            android:name="detailVijestiArgs"
            app:argType="hr.database.table.Vijesti" />
    </fragment>
    <fragment
        android:id="@+id/detailRasporedFragment"
        android:name="hr.fragmenti.detailFragmenti.DetailRasporedFragment"
        android:label="Detalji o rasporedu"
        tools:layout="@layout/detail_raspored_fragment">
        <argument
            android:name="detailRasporedArgs"
            app:argType="hr.database.table.Raspored" />
    </fragment>
    <fragment
        android:id="@+id/detailMomcadFragment"
        android:name="hr.fragmenti.detailFragmenti.DetailMomcadFragment"
        android:label="Detalji o momčadi"
        tools:layout="@layout/detail_momcad_fragment">
        <argument
            android:name="detailMomcadArgs"
            app:argType="hr.database.table.Igraci" />
    </fragment>
    <fragment
        android:id="@+id/detailRezultatiFragment"
        android:name="hr.fragmenti.detailFragmenti.DetailRezultatiFragment"
        android:label="Detalji o rezultatima"
        tools:layout="@layout/detail_rezultati_fragment">
        <argument
            android:name="detailRezultatArgs"
            app:argType="hr.database.table.Rezultat" />
    </fragment>
</navigation>